    <TextView android:layout_weight="5" android:text="TextView" android:id="@+id/buy_tv_market" android:layout_width="0px" android:layout_height="wrap_content"></TextView>
    <View android:layout_height="fill_parent" android:layout_width="1dp" android:background="#aaaaaa" />
    <TextView android:layout_weight="2" android:text="TextView" android:id="@+id/buy_tv_price_u" android:layout_width="0px" android:layout_height="wrap_content"></TextView>
	<View android:layout_height="fill_parent" android:layout_width="1dp" android:background="#aaaaaa" />
    <TextView android:layout_weight="2" android:text="TextView" android:id="@+id/buy_tv_price_e" android:layout_width="0px" android:layout_height="wrap_content"></TextView>
	</LinearLayout>
</TableLayout>
